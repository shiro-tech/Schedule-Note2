project#index
<div class="container">
  <div class="row">
    <div class="project-index-container">
      <h1 class="project-index">案件一覧</h1>
      <% @projects.each do |project| %>

        <p>
        <%= link_to project.title, project_path(id: project.id) %>
        </p>
      <% end %>
    </div>
  </div>
</div>
