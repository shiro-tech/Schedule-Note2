progress#index
<h1 class="progress-index">進捗情報一覧</h1>
<%= link_to 'progress-new', new_project_progress_path(project_id: params[:project_id]) %>
<%= link_to 'progress-comments-new', new_progress_comment_path %>
<% @progresses.each do |progress| %>
  <p>
    <%= link_to progress.title, edit_project_progress_path(project_id: params[:project_id],id: progress.id)  %>
  </p>

<% end %>
